<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" prefWidth="800" prefHeight="500" style="-fx-background-color: #F1EFEF;">
    <VBox spacing="10">
        <padding>
            <Insets top="20" right="20" bottom="20" left="20"/>
        </padding>

        <VBox spacing="5">
            <Text text="Agregar administrador" style="-fx-font-size: 24px; -fx-font-weight: bold;" />
            <Text text="Ingresa la información correctamente" style="-fx-font-size: 16px;" />
        </VBox>

        <GridPane hgap="20" vgap="15">
            <columnConstraints>
                <ColumnConstraints percentWidth="33"/>
                <ColumnConstraints percentWidth="33"/>
                <ColumnConstraints percentWidth="33"/>
            </columnConstraints>

            <!-- Primera fila -->
            <Label text="Nombre:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <TextField GridPane.columnIndex="0" GridPane.rowIndex="1"/>

            <Label text="Apellido paterno:" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="1"/>

            <Label text="Apellido materno:" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
            <TextField GridPane.columnIndex="2" GridPane.rowIndex="1"/>

            <!-- Segunda fila -->
            <Label text="Fecha de nacimiento:" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
            <TextField GridPane.columnIndex="0" GridPane.rowIndex="3"/>

            <Label text="Teléfono:" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="3"/>

            <Label text="Correo electrónico:" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
            <TextField GridPane.columnIndex="2" GridPane.rowIndex="3"/>

            
